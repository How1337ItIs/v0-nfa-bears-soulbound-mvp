{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "predev": "node scripts/mcp_preflight.js || exit 0",
    "dev": "next dev",
    "lint": "next lint",
    "prestart": "node scripts/mcp_preflight.js || exit 0",
    "start": "next start",
    "mcp:verify": "node scripts/verify_mcp_config.js",
    "mcp:fix": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/fix_mcp_config.ps1",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix",
    "lint:liquid-light": "eslint . --ext .ts,.tsx --config .eslintrc.js --plugin liquid-light",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:liquid-light": "jest --testPathPattern=liquid-light",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:visual": "jest --testPathPattern=visual",
    "visual:baseline:update": "node scripts/visual-baseline-update.js",
    "quality:check": "npm run type-check && npm run lint && npm run format:check && npm run test:coverage",
    "quality:fix": "npm run lint:fix && npm run format && npm run type-check",
    "quality:liquid-light": "npm run lint:liquid-light && npm run test:liquid-light",
    "pre-commit": "npm run quality:check",
    "pre-push": "npm run quality:check && npm run test:coverage",
    "clean": "rm -rf .next dist build coverage node_modules/.cache",
    "clean:all": "rm -rf .next dist build coverage node_modules/.cache node_modules package-lock.json",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "size-limit": "size-limit",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:fix": "npm audit fix",
    "liquid-light:build": "npm run type-check:strict && npm run lint:liquid-light && npm run test:liquid-light && npm run build",
    "liquid-light:dev": "npm run type-check && npm run lint:liquid-light && npm run dev",
    "liquid-light:test": "npm run test:liquid-light && npm run test:performance && npm run test:accessibility && npm run test:visual"
  },
